{
  "name": "voipservices-support-center",
  "version": "1.4.5",
  "description": "Centre de support VOIPServices avec gestion tickets et portabilités",
  "scripts": {
    "build": "cd frontend && yarn build",
    "dev": "cd frontend && yarn start",
    "dev:functions": "netlify dev",
    "version:patch": "node scripts/increment-version.js patch",
    "version:minor": "node scripts/increment-version.js minor",
    "version:major": "node scripts/increment-version.js major",
    "version:info": "node -e \"const {getVersionInfo} = require('./frontend/src/config/version'); console.log('Version actuelle:', getVersionInfo().version);\""
  },
  "dependencies": {
    "@netlify/neon": "^0.1.0",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2",
    "node-mailjet": "^6.0.6",
    "uuid": "^9.0.1",
    "express": "^4.18.2",
    "cors": "^2.8.5"
  },
  "devDependencies": {
    "netlify-cli": "^17.0.0"
  },
  "keywords": [
    "voip",
    "support",
    "tickets",
    "portabilités"
  ],
  "author": "VOIPServices",
  "license": "Proprietary"
}